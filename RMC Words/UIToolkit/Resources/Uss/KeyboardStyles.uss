.KeyboardKey {
    flex-grow: 1;
    margin-left: 2px;
    margin-right: 2px;
    margin-top: 2px;
    margin-bottom: 2px;
}

.KeyboardRow {
    flex-grow: 1;
    flex-shrink: 1;
    background-color: rgba(0, 0, 0, 0);
    flex-direction: row;
    width: 100%;
    height: 100%;
    align-items: auto;
    justify-content: flex-start;
    align-self: auto;
}

.Content {
    flex-grow: 1;
    background-color: rgba(0, 0, 0, 0);
}
